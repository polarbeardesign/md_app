<h1>Listing social_brands</h1>

<table>
  <tr>
    <th>Social brand name</th>
    <th>Brand icon</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @social_brands.each do |social_brand| %>
  <tr>
    <td><%= social_brand.social_brand_name %></td>
    <td><%= social_brand.brand_icon %></td>
    <td><%= link_to 'Show', social_brand %></td>
    <td><%= link_to 'Edit', edit_social_brand_path(social_brand) %></td>
    <td><%= link_to 'Destroy', social_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Social brand', new_social_brand_path %>
